<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module>
	<source path='ui'>
		<exclude name="widget/file/FileChooser.java" />
		<exclude name="widget/file/FileChooserAdapter.java" />
		<exclude name="widget/file/FileChooserListener.java" />
		<exclude name="widget/file/FileUtils.java" />
		<exclude name="widget/file/JNAFileDeleter.java" />
		<exclude name="widget/file/SingleFileChooserListener.java" />
		<exclude name="widget/file/StreamingFileChooserListener.java" />
        <exclude name="widget/file/internal/**" />
		<exclude name="util/form/FormValidator.java" />
		<exclude name="util/highlight/Highlighter.java" />
		<exclude name="util/highlight/RegexHighlightRule.java" />
		<exclude name="util/async/**" />
	</source>

    <!-- X1 skin -->
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/skin/x1/uiskin.json" />
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/skin/x1/uiskin.atlas" />
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/skin/x1/uiskin.png" />
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/skin/x1/default.fnt" />
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/skin/x1/font-small.fnt" />

    <!-- X2 skin -->
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/skin/x2/uiskin.json" />
    <extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/skin/x2/uiskin.atlas" />
    <extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/skin/x2/uiskin.png" />
    <extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/skin/x2/default.fnt" />
    <extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/skin/x2/font-small.fnt" />

    <!-- ColorPicker shaders -->
    <extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/widget/color/internal/default.vert" />
    <extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/widget/color/internal/hsv.frag" />
    <extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/widget/color/internal/rgb.frag" />
    <extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/widget/color/internal/palette.frag" />
    <extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/widget/color/internal/verticalBar.frag" />
    <extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/widget/color/internal/checkerboard.frag" />

    <!-- I18N files -->
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/i18n/ButtonBar.properties" />
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/i18n/ColorPicker.properties" />
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/i18n/Common.properties" />
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/i18n/Dialogs.properties" />
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/i18n/FileChooser.properties" />
	<extend-configuration-property name="gdx.files.classpath" value="com/kotcrab/vis/ui/i18n/TabbedPane.properties" />

    <!-- When changing those don't forget to update META-INF/robovm/ios/robovm.xml -->

	<extend-configuration-property name="gdx.reflect.include" value="com.kotcrab.vis.ui.Sizes" />
	<extend-configuration-property name="gdx.reflect.include" value="com.kotcrab.vis.ui.widget" />
	<extend-configuration-property name="gdx.reflect.include" value="com.kotcrab.vis.ui.widget.spinner" />
	<extend-configuration-property name="gdx.reflect.include" value="com.kotcrab.vis.ui.widget.dialog" />
	<extend-configuration-property name="gdx.reflect.include" value="com.kotcrab.vis.ui.widget.color" />
	<extend-configuration-property name="gdx.reflect.include" value="com.kotcrab.vis.ui.widget.tabbedpane" />
	<extend-configuration-property name="gdx.reflect.include" value="com.kotcrab.vis.ui.widget.toast" />
	<extend-configuration-property name="gdx.reflect.include" value="com.kotcrab.vis.ui.util.form" />
    <extend-configuration-property name="gdx.reflect.include" value="com.kotcrab.vis.ui.util.adapter.SimpleListAdapter" />

    <extend-configuration-property name="gdx.reflect.exclude" value="com.kotcrab.vis.ui.widget.file" />
    <extend-configuration-property name="gdx.reflect.include" value="com.kotcrab.vis.ui.widget.file.FileChooserStyle" />
</module>
